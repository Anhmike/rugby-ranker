<?xml version="1.0" encoding="utf-8"?>
<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/ic_news_black_24dp">

    <target android:name="largeInnerLine">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="180"
                android:startOffset="0"
                android:propertyName="trimPathEnd"
                android:valueFrom="1"
                android:valueTo="0" />
        </aapt:attr>
    </target>

    <target android:name="smallInnerLine">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="180"
                android:startOffset="180"
                android:propertyName="trimPathEnd"
                android:valueFrom="1"
                android:valueTo="0" />
        </aapt:attr>
    </target>

    <target android:name="smallInnerLine">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="180"
                android:startOffset="360"
                android:propertyName="trimPathEnd"
                android:valueFrom="0"
                android:valueTo="1" />
        </aapt:attr>
    </target>

    <target android:name="largeInnerLine">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="180"
                android:startOffset="540"
                android:propertyName="trimPathEnd"
                android:valueFrom="0"
                android:valueTo="1" />
        </aapt:attr>
    </target>

</animated-vector>
