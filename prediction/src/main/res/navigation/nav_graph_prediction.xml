<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/predictionNavGraph"
    app:startDestination="@id/predictionBottomSheetDialogFragment"
    tools:ignore="UnusedNavigation">

    <dialog
        android:id="@+id/predictionBottomSheetDialogFragment"
        android:name="com.ricknout.rugbyranker.prediction.ui.PredictionBottomSheetDialogFragment"
        android:label="PredictionBottomSheetDialogFragment"
        tools:layout="@layout/bottom_sheet_dialog_fragment_prediction">
        <argument android:name="sport" app:argType="com.ricknout.rugbyranker.core.vo.Sport" />
        <argument android:name="isEditing" app:argType="boolean" />
        <argument android:name="prediction" app:nullable="true" app:argType="com.ricknout.rugbyranker.prediction.vo.Prediction" />
    </dialog>

</navigation>
